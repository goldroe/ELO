Token_Kind :: enum {
 Eof,
 Name,
 Int,
 Float,
 String,
 Plus,
 Minus,
}

Vector2 :: struct {
 x: f32;
 y: f32;
}

Vector3 :: struct {
 x: f32;
 y: f32;
 z: f32;
}

operator + :: (a: Vector2, b: Vector2) -> Vector2 {
 result : Vector2 = .{a.x + b.x, a.y + b.y};
 return result;
}

operator + :: (a: Vector3, b: Vector3) -> Vector3 {
 result : Vector3 = .{a.x + b.x, a.y + b.y, a.z + b.z};
 return result;
}

Fruit :: struct {
 calories: s32;
 nutrients: s32;
 buzz: Buzz;
}

Buzz :: struct {
 bar: s32;
}

Foo :: struct {
